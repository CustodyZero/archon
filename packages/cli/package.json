{
  "name": "@archon/cli",
  "version": "0.0.1",
  "description": "Archon operator CLI",
  "type": "module",
  "bin": {
    "archon": "./dist/bin/archon.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "dependencies": {
    "@archon/kernel": "workspace:*",
    "@archon/module-filesystem": "workspace:*",
    "@archon/module-loader": "workspace:*",
    "@archon/runtime-host": "workspace:*",
    "chalk": "^5.6.2",
    "commander": "^12.1.0",
    "ink": "^6.8.0",
    "react": "^19.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.14",
    "typescript": "^5.5.0"
  }
}
